<p id="notice"><%= notice %></p>

<h1>Liked Locations</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @liked_locations.each do |liked_location| %>
      <tr>
        <td><%= liked_location.user_id %></td>
        <td><%= liked_location.location_id %></td>
        <td><%= link_to 'Show', liked_location %></td>
        <td><%= link_to 'Edit', edit_liked_location_path(liked_location) %></td>
        <td><%= link_to 'Destroy', liked_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Liked Location', new_liked_location_path %>
